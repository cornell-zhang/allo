{
    "name": "top", 
    "description": ["Allo generated kernels"],
    "flow": "vitis",
    "keywords": [], 
    "key_concepts": [],
    "platform_blocklist": [
        "nodma"
    ], 
    "os": [
        "Linux"
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "host": {
        "host_exe": "top",
        "compiler": {
            "sources": [
                "xcl2.cpp",
                "host.cpp"
            ], 
            "includepaths": [
                "."
            ]
        }
    },
    "containers": [
        {
            "accelerators": [
                {
                    "name": "top", 
                    "location": "kernel.cpp"
                }
            ], 
            "name": "top",
            "ldclflags": "--optimize 3"
        }
    ],
    "launch": [
        {
            "cmd_args": "BUILD/top.xclbin", 
            "name": "generic launch for all flows"
        }
    ], 
    "contributors": [
        {
            "url": "https://github.com/cornell-zhang/allo", 
            "group": "Allo"
        }
    ],
    "testinfo": {
        "disable": false,
        "profile": "no",
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 32768,
                "max_time_min": 300
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw"
        ],
        "category": "canary"
    }
}
